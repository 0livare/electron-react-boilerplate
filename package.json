{
  "name": "pbi-tips-desktop",
  "version": "0.0.1",
  "private": true,
  "main": "src/electron/main.js",
  "description": "A desktop application for easily adding external tools to PowerBI Desktop",
  "productName": "PbiTipsDesktop",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm-run-all --parallel start-react start-electron",
    "start-react": "react-scripts start",
    "start-electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "build-exe": "node bld/build-exe",
    "build-admin-exe": "node bld/give-exe-admin-rights",
    "build-installer": "node bld/build-installer",
    "installer": "npm-run-all --serial clean build-exe build-admin-exe build-installer"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "electron": "^9.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "electron-packager": "^15.0.0",
    "electron-winstaller": "^4.0.1",
    "execa": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
